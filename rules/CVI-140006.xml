<?xml version="1.0" encoding="UTF-8"?>
<cobra document="https://github.com/WhaleShark-Team/cobra">
    <name value="直接输出入参可能导致XSS"/>
    <language value="java"/>
    <match mode="function-param-controllable"><![CDATA[(request.setAttribute)]]></match>
    <level value="4"/>
    <test>
        <case assert="true"><![CDATA[request.setAttribute("test")]]></case>
    </test>
    <solution>
        ## 安全风险
        直接输出入参会导致XSS

        ## 修复方案
        使用Begis安全组件对参数进行过滤后使用
    </solution>
    <status value="on"/>
    <author name="BlBana" email="blbana@qq.com"/>
</cobra>